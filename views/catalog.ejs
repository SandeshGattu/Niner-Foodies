<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
  <meta charset="utf-8">
  <title>Niner Foodsite</title>
   <link rel="icon" type="image/png" href="/images/backg.png" />
   <link rel="stylesheet" href="/stylesheets/style.css">
</head>
  <body>

    <% include partials/header%>
    <% include partials/user-navigation%>

    <div class="horiz">
         <% include partials/site-navigation.ejs %>
         <div class="vertic content">
 <p> <a href="index">Home </a> > categories</p>
            <h6><%= data.list[0]%></h6>
            <ul>
              <% for(var i = 0; i< data.items.length; i++ ){
                if(data.list[0] == data.items[i].itemCategory){%>
                  <li>
                    <h4><a href="../catalog/item/<%= data.items[i].itemCode%>"><%= data.items[i].itemName%></a></h4></li>
                <%}
              }%>
            </ul>
            <h6><%= data.list[1]%></h6>
            <ul>
              <% for(var j = 0; j< data.items.length; j++ ){
                if(data.list[1] == data.items[j].itemCategory){%>
                  <li><h4><a href="../catalog/item/<%= data.items[j].itemCode%>"><%= data.items[j].itemName%></a></h4></li>
                <%}
              }%>
            </ul>
          </div>
   </div>
  </body>      <% include partials/footer%>



</html>
